{
  "manifest_version": 2,

  "name": "Chrome DevX Launcher",
  "description": "Simple page launcher for sites running locally.",
  "version": "1.0",
  "omnibox": { "keyword" : "devx" },
  "options_page": "options.html",
  "permissions": [
    "notifications",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "error.png",
    "success.png"
  ]
}